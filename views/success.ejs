    <table class="table text-center">
      <thead class="thead-dark">
        <tr>
          <th scope="col">Account #</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Password</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <%= userInfo[0] %>
          </td>
          <td>
            <%= userInfo[1] %>
          </td>
          <td>
            <%= userInfo[2] %>
          </td>
          <td>
            <%= userInfo[3] %>
          </td>
      </tbody>
    </table>
    <a href="/" class="btn btn-primary">Home</a>
  </div>

</body>

</html>